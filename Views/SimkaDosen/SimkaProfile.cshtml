        function GetArtikelPublikasi(id_dokumen) {
            //window.open('/SimkaAdmin/unduhDokumen/' + id_dokumen, "_blank");
            //window.location.href = "/SimkaDosen/unduhDokumen/" + id_dokumen
            $.ajax({
                type: 'get',
                url: '/SimkaDosen/GetArtikelPublikasi/' + id_dokumen,
                contentType: "application/pdf",
                xhrFields: { responseType: "arraybuffer"},
                success: function (data) {


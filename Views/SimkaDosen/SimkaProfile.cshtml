                },
                error: function (data) {
                    console.log(data)

                }
            });
        }

        function GetDokumenPendukungPublikasi(id_publikasi)
        {
            $('#id_publikasi').val(id_publikasi);
            console.log("ini get dokumen Pendukung")
            $.ajax({
                type: "GET",
                url: "/SimkaDosen/GetDokumenPendukungPublikasi?id_publikasi=" + id_publikasi,
                dataType: "json",
                success: function (data) {
                    if (data.file_prosiding != null) {
                       
                        var fileUrl = "data:application/pdf ;base64," + data.file_prosiding;
                        fetch(fileUrl)
                            .then(response => response.blob())
                            .then(blob => {
                                var blobUrl = URL.createObjectURL(blob);

                                $('#previewProsiding').html(
                                    "<iframe src='" + blobUrl + "' style='width:100%; height:400px;'></iframe>"
                                );
                            });
                    } else {
                        $('#previewProsiding').html("<span>File belum diupload</span>");
                    }

                    if (data.file_cek_similaritas != null) {
                        
                        var fileUrl = "data:application/pdf ;base64," + data.file_cek_similaritas;
                        fetch(fileUrl)
                            .then(response => response.blob())
                            .then(blob => {
                                var blobUrl = URL.createObjectURL(blob);

                                $('#previewCekSimilaritas').html(
                                    "<iframe src='" + blobUrl + "' style='width:100%; height:400px;'></iframe>"
                                );
                            });
                    } else {
                        $('#previewCekSimilaritas').html("<span>File belum diupload</span>");
                    }

                    if (data.file_peer_review_pak != null) {
                        
                        var fileUrl = "data:application/pdf ;base64," + data.file_peer_review_pak;
                        fetch(fileUrl)
                            .then(response => response.blob())
                            .then(blob => {
                                var blobUrl = URL.createObjectURL(blob);

                                $('#previewPAK').html(
                                    "<iframe src='" + blobUrl + "' style='width:100%; height:400px;'></iframe>"
                                );
                            });
                    } else {
                        $('#previewPAK').html("<span>File belum diupload</span>");
                    }

                    if (data.file_peer_korespondensi != null) {
                       
                        var fileUrl = "data:application/pdf ;base64," + data.file_peer_korespondensi;
                        fetch(fileUrl)
                            .then(response => response.blob())
                            .then(blob => {
                                var blobUrl = URL.createObjectURL(blob);

                                $('#previewKorespondensi').html(
                                    "<iframe src='" + blobUrl + "' style='width:100%; height:400px;'></iframe>"
                                );
                            });
                    } else {
                        $('#previewKorespondensi').html("<span>File belum diupload</span>");
                    }

                   

                    console.log(data);
                },
                error: function (data) {
                    console.log("get dokumen pendukung error")
                }
            });
        }

        function PostDokumenPendukung() {
            console.log("post dokumen Pendukung")
            var data = new FormData();
            jQuery.each(jQuery('#file_prosiding')[0].files, function (i, file) {
                data.append('file_prosiding', file);
            });
            jQuery.each(jQuery('#file_cek_similaritas')[0].files, function (i, file) {
